						"          cod_gerencia as string,",
						"          nom_gerencia as string,",
						"          Ano_venda as string,",
						"          {Mês_venda} as string,",
						"          Dia_venda as string,",
						"          dtc_venda as string,",
						"          qtd_venda as string,",
